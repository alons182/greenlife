@font-face {
	font-family: 'greenlife';
	src:url('../fonts/greenlife.eot');
	src:url('../fonts/greenlife.eot?#iefix') format('embedded-opentype'),
		url('../fonts/greenlife.ttf') format('truetype'),
		url('../fonts/greenlife.woff') format('woff'),
		url('../fonts/greenlife.svg#greenlife') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'greenlife';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-next:before {
	content: "\e600";
}
